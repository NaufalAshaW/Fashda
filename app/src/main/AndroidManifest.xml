<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nasha.fashda">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_window"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_window"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fashda">
        <activity android:name=".WalkthroughActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".MosqueDetailActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".SplashActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nasha.fashda.MainActivity"/>
    </application>

</manifest>